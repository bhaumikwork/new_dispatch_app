<script src="https://maps.googleapis.com/maps/api/js">
    </script>
<div id="tracking-result">
	<% if @location_detail.is_reached %>
		<%= render partial: 'reached_to_destination' %>
	<% else %>
		<%= render partial: 'tracking_result' %>
	<% end %>
</div>
<script type="text/javascript">
	<% unless dispatcher_signed_in? %>
	   window.onbeforeunload = function () {
	    return ' The following action will cause you to lose all displayed data...are you sure you wish to continue?';
	  }
	<% else %>
		getLocation();
	<% end %>
</script>
